


{{# each products }}
    <div class="row">
        {{# each this }}
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="{{ this.imagePath }}" alt="..." class="img-responsive">
                    <div class="caption">
                        <h3>{{ this.title }}</h3>
                        <div class="clearfix">
                            <div class="price pull-left">${{ this.price }}</div>
                            <a href="/add-to-food-cart/{{ this._id }}" class="btn btn-primary pull-right" role="button">Adding to food cart </a> 
   
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
{{/each}}


